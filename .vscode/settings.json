{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.fixAll.eslint": true
  },
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "eslint.validate": ["typescript", "svelte"],
  "files.eol": "\n",
  "svelte.enable-ts-plugin": true
}
